<node name="/org/amberapi/zxing">
  <interface name="org.amberapi.zxing">
    <method name="decodeFromDescriptor">
      <arg direction="out" type="s" name="result"/>
      <arg direction="in" type="h" name="descriptor"/>
      <arg direction="in" type="u" name="buffer_size"/>
      <arg direction="in" type="i" name="width"/>
      <arg direction="in" type="i" name="height"/>
      <arg direction="in" type="i" name="pixelFormat"/>
    </method>
    <method name="quit">
    </method>
   </interface>
</node>
